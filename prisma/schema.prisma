// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model KeyVal {
  name  String @unique
  value String
}

model Match {
  id          String   @id
  startTime   DateTime
  radiantWin  Boolean
  duration    Int
  teamRadient Bytes    @map("team1")
  teamDire    Bytes    @map("team2")

  @@index([teamRadient])
  @@index([teamDire])
}
